{
    "name": "degenhf/ecc-auth-laravel",
    "description": "ECC-based authentication for Laravel with enhanced security",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DegenHF",
            "email": "degenhf@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "ext-openssl": "*",
        "ext-sodium": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "DegenHF\\EccAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DegenHF\\EccAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DegenHF\\EccAuth\\EccAuthServiceProvider"
            ],
            "aliases": {
                "EccAuth": "DegenHF\\EccAuth\\Facades\\EccAuth"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}